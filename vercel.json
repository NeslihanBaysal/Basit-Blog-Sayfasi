{
  "routes": [
    {
      "src": "/",
      "dest": "/blog-project/index.html"
    },
    {
      "src": "/the-blog",
      "dest": "/blog-project/the-blog.html"
    },
    {
      "src": "/src/css/(.*)",
      "dest": "/blog-project/src/css/$1"
    },
    {
      "src": "/src/img/(.*)",
      "dest": "/blog-project/src/img/$1"
    },
    {
      "src": "/dist/(.*)",
      "dest": "/blog-project/dist/$1"
    }
  ]
}
