{
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "the-blog.html",
      "use": "@vercel/static"
    },
    {
      "src": "blog-project/src/css/(.*)",
      "use": "@vercel/static"
    },
    {
      "src": "blog-project/src/img/(.*)",
      "use": "@vercel/static"
    },
    {
      "src": "blog-project/dist/(.*)",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/the-blog",
      "dest": "/the-blog.html"
    },
    {
      "src": "/blog-project/src/css/(.*)",
      "dest": "/blog-project/src/css/$1"
    },
    {
      "src": "/blog-project/src/img/(.*)",
      "dest": "/blog-project/src/img/$1"
    },
    {
      "src": "/blog-project/dist/(.*)",
      "dest": "/blog-project/dist/$1"
    }
  ]
}
