{
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/the-blog",
      "dest": "/the-blog.html"
    },
    {
      "src": "/src/css/(.*)",
      "dest": "/src/css/$1"
    },
    {
      "src": "/src/img/(.*)",
      "dest": "/src/img/$1"
    },
    {
      "src": "/dist/(.*)",
      "dest": "/dist/$1"
    }
  ]
}
